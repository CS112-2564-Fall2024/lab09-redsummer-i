<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<Pane fx:id="mapPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs112.lab09.controllers.MapController">
    <ImageView fx:id="mapImageView" fitHeight="500.0" fitWidth="500.0">
        <image>
            <Image url="@../resources/images/Red-Summer.jpg"/>
        </image>
    </ImageView>
    <Button fx:id="sanFranciscoButton" text="San Francisco, CA" layoutX="50.0" layoutY="100.0" onAction="#handleSanFranciscoEvent"/>
    <Button fx:id="bisbeeAzButton" text="Bisbee, AZ" layoutX="200.0" layoutY="200.0" onAction="#handleBisbeeEvent"/>
</Pane>
